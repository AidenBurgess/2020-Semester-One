\end_layout

\begin_layout Plain Layout

Year CHAR(2) NOT NULL,
\end_layout

\begin_layout Plain Layout

Instructor VARCHAR(20) NOT NULL,
\end_layout

\begin_layout Plain Layout

PRIMARY KEY (Section_identifier),
\end_layout

\begin_layout Plain Layout

FOREIGN KEY (Course_number) REFERENCES COURSE(Course_number),
\end_layout

\begin_layout Plain Layout

);
\end_layout

\end_inset


\end_layout

