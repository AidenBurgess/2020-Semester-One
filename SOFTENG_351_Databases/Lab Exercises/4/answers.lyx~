\end_layout

\begin_layout Plain Layout

);
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "language=SQL,numbers=left"
inline false
status open

\begin_layout Plain Layout

CREATE TABLE PREREQUISITE (
\end_layout

\begin_layout Plain Layout

Course_number VARCHAR(14) NOT NULL,
\end_layout

\begin_layout Plain Layout

Prerequisite_number VARCHAR(14) NOT NULL,
\end_layout

\begin_layout Plain Layout

PRIMARY KEY (Course_number, Prerequisite_number),
\end_layout

\begin_layout Plain Layout

FOREIGN KEY (Course_number) REFERENCES COURSE(Course_number),
\end_layout

\begin_layout Plain Layout

FOREIGN KEY (Prerequisite_number) REFERENCES COURSE(Course_number)
\end_layout

\begin_layout Plain Layout

);
\end_layout

\end_inset


\end_layout

