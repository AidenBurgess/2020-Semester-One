\begin_layout Plain Layout

SELECT Dno, Dname, COUNT(*) AS No_of_Employees, SUM(Salary) AS Total_Sal,
 MAX(Salary) AS Highest_Sal, MIN(Salary) AS Lowest_Sal, AVG(Salary) AS Average_S
al FROM EMPLOYEE NATURAL JOIN DEPARTMENT WHERE Dno=Dnumber GROUP BY Dno
\end_layout

