SELECT S.year, S.term, S.cprefix, S.cno, C.ctitle, E.grade FROM enrolls AS E,
 sections as S, courses as C WHERE E.crn=S.crn AND E.term=S.term AND E.year=S.year
 AND S.cno=C.cno AND E.sid="1111"
