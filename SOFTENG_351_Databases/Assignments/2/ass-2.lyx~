\begin_layout Plain Layout

SELECT crn, ctitle, C.cprefix, C.cno, sid, fname, lname FROM courses AS C,
 students, sections AS S WHERE C.cno=S.cno AND S.crn="10101" AND S.term="FA"
 AND S.year="2002"
\end_layout

\end_inset


\end_layout

