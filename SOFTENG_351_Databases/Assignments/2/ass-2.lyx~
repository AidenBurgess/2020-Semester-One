\begin_layout Plain Layout

SELECT DISTINCT E.crn, C.ctitle, S.cprefix, S.cno, S.days, S.startTime, S.endTime,
 S.room, S.instructor FROM enrolls AS E, sections AS S, courses AS C WHERE
 E.sid="1111" AND E.term="FA" AND E.year="2002" AND C.cno=S.cno AND E.crn=S.crn
\end_layout

\end_inset


