public class NetworkLink {
	private String _src;
	private String _dst;
	private long _cost;

	// TODO: include the current cost and predecessor
	
	public NetworkLink(String src, String dst, long cost) {
		_src = src;
		_dst = dst;
		_cost = cost;
	}
	
	public String getSrc() {
		return _src;
	}
	
	public String getDst() {
		return _dst;
	}
	
	public long getCost() {
		return _cost;
	}
	// TODO: include getters and setters for the current cost and predecessor
}
